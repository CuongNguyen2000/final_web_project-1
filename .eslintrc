{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended", "google"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "new-cap": "off",
    "max-len": "warn",
    "no-unused-vars": "warn"
  },
  "parser": "babel-eslint"
}
